	cd ~/rpmbuild/SPECS
	rpmbuild -ba mysql++.spec

ebuild: dist
	cp @PACKAGE_NAME@-@PACKAGE_VERSION@.tar.gz /usr/portage/distfiles
	cp mysql++.ebuild /usr/portage/dev-db/mysql++/@PACKAGE_NAME@-@PACKAGE_VERSION@.ebuild
	ebuild /usr/portage/dev-db/mysql++/@PACKAGE_NAME@-@PACKAGE_VERSION@.ebuild digest

